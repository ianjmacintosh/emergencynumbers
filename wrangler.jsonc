  "name": "emergencynumbers",
  "main": "./worker/index.ts",
  "compatibility_date": "2026-02-05",
  "assets": {
    "not_found_handling": "single-page-application",
    "run_worker_first": ["/api/*"],
  },
  "workers_dev": true,
  "routes": [
    {
      "pattern": "emergencynumbers.info",
      "custom_domain": true,
    },
    {
      "pattern": "emergencynumbers.ianjmacintosh.com",
      "custom_domain": true,
    },
  ],
}
