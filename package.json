{
  "name": "emergencynumbers",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview --host",
    "preview:cf": "npm run build && npx wrangler versions upload",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc -b",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest",
    "test:e2e": "npx playwright test --reporter=list",
    "convert:itu": "tsx scripts/convert-itu-xls.ts src/assets/ITU-T_Report_2026-02-13-2200.xls > src/constants/emergency-services.ts"
  },
  "dependencies": {
    "@ariakit/react": "^0.4.21",
    "@phosphor-icons/react": "^2.1.10",
    "country-flag-icons": "^1.6.13",
    "match-sorter": "^8.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.23.1",
    "@cloudflare/workers-types": "^4.20260227.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18",
    "wrangler": "^4.63.0"
  }
}
